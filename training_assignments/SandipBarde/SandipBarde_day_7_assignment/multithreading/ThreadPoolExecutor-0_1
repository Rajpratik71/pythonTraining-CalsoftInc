                expires = new Date();

                if (paramMap[type]) {
                    expires.setMinutes(expires.getMinutes() + parseInt(1,10));
                    Y.Cookie.set('autorf', paramMap[type], {expires: expires, domain: location.hostname});

                    if (isModalOpen) {
                        Y.config.win.location = url
                    } else {
                        Y.config.win.location.replace(url);
                    }
                }
            }
